fnc print(a : str, ...) : void;

enum Fruit {
    Apple,
    Banana,
    Orange,
}

fnc main() : i32 {
    my_fruit : Fruit = Fruit::Apple;
    match (my_fruit) {
        case Fruit::Apple => {
            print("Apple");
        }
        case Fruit::Banana => {
            print("Banana");
        }
        case Fruit::Orange => {
            print("Orange");
        }
    }
    ret 0;
}
